{
  "name": "Scriptee",
  "displayName": "Scriptee",
  "publisher": "Scripty",
  "description": "scripty",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "scripty.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "scripty.analyzeCode",
        "title": "Analyze Code"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/estree": "^1.0.5",
    "@types/json-schema": "^7.0.15",
    "@types/mocha": "^10.0.7",
    "@types/node": "^22.5.5",
    "@types/vscode": "^1.93.0",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.9.1",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "odbc": "^2.4.9"
  }
}
